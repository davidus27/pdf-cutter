{
  "name": "pdf-cutter",
  "version": "0.2.0",
  "description": "Browser extension for making new pdf files containing only pages with highlighted content",
  "main": ".eslintrc.js",
  "dependencies": {
    "pdf-lib": "^1.16.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.19.0",
    "html-webpack-plugin": "^4.5.1",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.7.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --open --config webpack.dev.js; sass ./src/popup/styles/styles.scss:./src/popup/styles/styles.css",
    "build": "webpack --config webpack.prod.js; sass ./src/popup/styles/styles.scss:./src/popup/styles/styles.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidus27/pdf-cutter.git"
  },
  "keywords": [
    "pdf",
    "highlighting",
    "chrome",
    "extension"
  ],
  "author": "David Drobny",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidus27/pdf-cutter/issues"
  },
  "homepage": "https://github.com/davidus27/pdf-cutter#readme"
}
