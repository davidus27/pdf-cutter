{
    "manifest_version" : 2,
    "name": "PDF Highlight extraction | My first extension",
    "version": "0.1.0",
    "browser_action": {
        "default_icon": "./imgs/icon.png",
        "default_popup": "popup.html"
      },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./js/content.js"]
        }
    ],
    "background": {
            "scripts": ["./js/background.js"]
    },
    "permissions": [
        "storage",
        "unlimitedStorage"
    ]
}